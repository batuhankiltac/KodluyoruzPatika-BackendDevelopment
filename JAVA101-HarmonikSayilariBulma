import java.util.*;

public class Main {
    public static void main(String[] args) {

        double i;
        double result = 0;

        Scanner input = new Scanner(System.in);

        System.out.print("Bir sayı girin: ");
        i = input.nextInt();

        while (i > 0) {
            result += (1 / i);
            i--;
        }
        System.out.println("Sonuç = " + result);
    }
}
