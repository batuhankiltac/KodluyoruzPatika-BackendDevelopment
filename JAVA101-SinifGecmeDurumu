import java.util.*;

public class Main {
    public static void main(String[] args) {
        int matematik;
        int fizik;
        int turkce;
        int kimya;
        int muzik;
        double ortalama;
        boolean kontrol;

        Scanner input = new Scanner(System.in);

        System.out.print("Matematik dersi notunu giriniz: ");
        matematik = input.nextInt();

        System.out.print("Fizik dersi notunu giriniz: ");
        fizik = input.nextInt();

        System.out.print("Türkçe dersi notunu giriniz: ");
        turkce = input.nextInt();

        System.out.print("Kimya dersi notunu giriniz: ");
        kimya = input.nextInt();

        System.out.print("Müzik dersi notunu giriniz: ");
        muzik = input.nextInt();

        ortalama = (matematik + fizik + turkce + kimya + muzik) / 5;

        kontrol = (matematik > 0 && matematik < 100) && (fizik > 0 && fizik < 100) &&
                (turkce > 0 && turkce < 100) && (kimya > 0 && kimya < 100) &&
                (muzik > 0 && muzik < 100);

        if (ortalama >= 55 && kontrol == true) {
            System.out.println("Tebrikler " + ortalama + " ortalama ile geçtiniz");
        }
        else if (kontrol == false) {
            System.out.println("Not girişi 0-100 arası olmalıdır.");
        }
        else {
            System.out.println("Üzgünüz " + ortalama + " ortalama ile kaldınız.");
        }
    }
}
